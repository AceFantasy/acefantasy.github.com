&lt;/dd&gt;
&lt;dt style=&quot;border-left: 4px solid rgb(215, 81, 0); padding-left: 5px; margin: 5px auto;&quot;&gt;&lt;i style=&quot; color: #d75100; font-style: normal; &quot;&gt;$.grep()&lt;/i&gt;&lt;/dt&gt;&lt;dd style=&quot;margin-left: 30px;&quot;&gt;
