&lt;h2&gt;&#29983;&#25104;&#38543;&#26426;&#23383;&#31526;&#20018;&lt;/h2&gt;
&lt;p&gt;&#38271;&#24230;&#65306;&lt;input id="random_str_length" value="64" style="width: 30px;"/&gt;&lt;button id="random_str_btn" style="margin-left: 5px; margin-right: 20px;"&gt;&#30830;&#23450;&lt;/button&gt;&lt;/p&gt;

&lt;xmp id="random_str_result" style="color: red;"&gt;&#12288;&lt;/xmp&gt;
        
        &lt;div class="end_mark"&gt;&lt;/div&gt;

        &lt;/div&gt;
    &lt;/div&gt;

&lt;script type="text/javascript" charset="utf-8"&gt;

var _CHAR_POOL = '!#%&amp;`()*+,-./:;&lt;=&gt;?@[\]^_{}~01234567890abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ';
var get_token = dojo.hitch({char_pool: _CHAR_POOL, len: _CHAR_POOL.length}, function(len){
    if(!(len &amp;&amp; parseInt(len))){len = 64}
    var s = '';
    for(var i = 0; i &lt; len; i++){
        var r = parseInt(Math.random() * 1000) % (this.len);
        s += this.char_pool[r];
    }
    return s;
});

dojo.addOnLoad(function(){
    //&#29983;&#25104;&#38543;&#26426;&#23383;&#31526;&#20018;
    (function(){
        dojo.connect(dojo.byId('random_str_btn'), 'onclick', {f: get_token},
            function(eventObj){
                var len = dojo.byId('random_str_length').value;
                len = parseInt(len);
                dojo.byId('random_str_result').innerHTML = this.f(len);
            }
        );
    })();        
});    

&lt;/script&gt;

